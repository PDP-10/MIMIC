	MOVSI	%03,(POINT 6,%00,)
CRFOU1:	ILDB	%02,%03		;GET A SIXBIT CHARACTER
	JUMPE	%02,CRFOU2	;BRANCH IF END
	ADDI	%02,40		;CONVERT TO ASCII
	PUSHJ	%17,LSTDMP	;LIST IT
	TLNE	%03,770000	;END OF WORD?
	JRST	CRFOU1		;  NO, GET ANOTHER
CRFOU2:	POPJ	%17,0		;EXIT

CRFTBL:				;CREF TABLE
