PAL8:	MOVE	%00,[XWD BZCOR,BZCOR+1]
	SETZB	%01,BZCOR
	BLT	%00,EZCOR-1
	RUNTIM	%01,
	MOVEM	%01,RUNTIM
	DATE	%00,
	MOVEM	%00,DATE	;SAVE DATE
	MSTIME	%00,
	MOVEM	%00,MSTIME	;  AND TIME
	MOVSI	%16,BINBIT!LSTBIT!CSWBIT
	HRRI	%16,HDRBIT
	MOVSI	%15,P1F		;SET FOR PASS1

	MOVE	%00,[INIT TTI,1]
	MOVSI	%01,(SIXBIT /TTY/)
