GRILNK	GRI-909 LINK EDITOR	MACRO 47(113) 16:15  8-MAR-73 PAGE 3
GLA	MAC	 7-NOV-72 17:51		DICTIONARY PROCESSING

						SUBTTL DICTIONARY PROCESSING
					;GRI-909 RELOCATING LOADER, PAGE 3
					;DICTN:  PROCESS A DICTIONARY ENTRY.
					;ON ENTRY'S-- MULTIPLY DEFINED ENTRY'S ARE FLAGGED AND IGNORED
					;  NEW ENTRY'S ARE USED TO RESOLVE OUTSTANDING EXTERNS AND ARE
					;  STORED IN THE MASTER SYMBOL TABLE.
					;ON EXTERN'S-- IF A MATCHING ENTRY ALREADY EXISTS, THE EXTERN IS
					;  RESOLVED ON THE SPOT.  OTHERWISE IT IS PUT IN THE MASTER
					;  SYMBOL TABLE.

	400056'	260 17 0 00 400171'	DICTN:	PUSHJ P,GETRHD	;DICTIONARY BLOCK.  GET HEADER.
	400057'	254 00 0 00 400342'		JRST BADBLK
	400060'	260 17 0 00 400221'	DICTLP:	PUSHJ P,RDSYM	;GET NEXT SYMBOL ENTRY.
	400061'	254 00 0 00 400342'		JRST BADBLK
	400062'	603 07 0 00 020000		TLNE FLGS,QENT	;IS IT AN ENTRY?
	400063'	254 00 0 00 400107'		JRST ENTFND	;YES, GO PROCESS.
	400064'	602 07 0 00 030000	EXTFND:	TRNE FLGS,GRIPLS+GRIMIN	;IS EXTERN EITHER A PLUS OR MINUS REF?
	400065'	254 00 0 00 400070'		JRST NOTDGN	;NO, CANNOT BE DEGENERATE CASE.
	400066'	306 06 0 00 177777		CAIN VALUE,177777	;IS VALUE 177777?
	400067'	254 00 0 00 400101'		JRST COUNT	;YES, IGNORE EXTERN.
	400070'	200 13 0 00 000005	NOTDGN:	MOVE A,NAME	;NO, AN EXTERN.  LOOK IT UP IN THE MASTER
	400071'	260 17 0 00 000000*		PUSHJ P,$LKGLBL	;SYMBOL TABLE OF GLOBALS.
	400072'	254 00 0 00 400103'		JRST INWREF	;NOT PRESENT IN ANY FORM.  GO INSERT IT.
	400073'	200 01 0 13 000002		MOVE AC,SYMF(A)	;FOUND.  IS TABLE SYMBOL ALSO AN
	400074'	603 01 0 00 040000		TLNE AC,QEXT	;EXTERN REFERENCE?
	400075'	254 00 0 00 400103'		JRST INWREF	;YES, GO INSERT NEW REFERENCE.
	400076'	200 03 0 00 000013		MOVE ENT,A	;NO, ENTRY IN TABLE IS A TRUE ENTRY.
	400077'	201 04 0 00 000005		MOVEI EXT,NAME	;SYMBOL IN "NAME" IS AN EXTERN.
	400100'	260 17 0 00 400123'		PUSHJ P,RESOLV	;GP RESOLVE EXTERN REFERENCE.
	400101'	367 11 0 00 400060'	COUNT:	SOJG CNT,DICTLP	;DECREMENT BLOCK COUNT.  LOOP BACK IF NOT DONE.
	400102'	254 00 0 00 400006'		JRST LOADRC	;IF DONE, GO CHECKSUM.

	400103'	201 13 0 00 000005	INWREF:	MOVEI A,NAME	;INSERT CURRENT SYMBOL INTO
	400104'	260 17 0 00 000000*		PUSHJ P,$STSYM	;MASTER SYMBOL TABLE.
	400105'	254 00 0 00 400350'		JRST NOCORE	;ERROR
	400106'	254 00 0 00 400101'		JRST COUNT	;GO DECREMENT BLOCK COUNT.

	400107'	200 13 0 00 000005	ENTFND:	MOVE A,NAME	;SYMBOL IS AN ENTRY.  LOOK IT UP.
	400110'	260 17 0 00 400071*		PUSHJ P,$LKGLBL	;IF NOT FOUND, GO INSERT.
	400111'	254 00 0 00 400103'		JRST INWREF
	400112'	200 01 0 13 000002		MOVE AC,SYMF(A)	;FOUND, GET FLAGS FROM SYMBOL TABLE.
	400113'	603 01 0 00 020000		TLNE AC,QENT	;ALSO AN ENTRY>
	400114'	254 00 0 00 400313'		JRST MULDEF	;YES, MULTIPLY DEFINED.
	400115'	201 03 0 00 000005		MOVEI ENT,NAME	;NO.  EXTERN IS IN TABLE, ENTRY IS IN
	400116'	200 04 0 00 000013		MOVE EXT,A	;CURRENT SYMBOL.
	400117'	260 17 0 00 400123'		PUSHJ P,RESOLV	;GO RESOLVE.
	400120'	200 13 0 00 000004		MOVE A,EXT	;PURGE THE EXTERN REFERENCE FROM
	400121'	260 17 0 00 000000*		PUSHJ P,$PURGE	;THE MASTER SYMBOL TABLE.
	400122'	254 00 0 00 400107'		JRST ENTFND	;TRY AGAIN.
GRILNK	GRI-909 LINK EDITOR	MACRO 47(113) 16:15  8-MAR-73 PAGE 4
GLA	MAC	 7-NOV-72 17:51		DICTIONARY PROCESSING

					;GRI-909 RELOCATING LOADER, PAGE 4
					;RESOLVE AN ENTRY-EXTERN PAIR.
					;ENT	POINTS TO ENTRY SYMBOL BLOCK.
					;EXT	POINTS TO EXTERN SYMBOL BLOCK.

	400123'	200 01 0 04 000001	RESOLV:	MOVE AC,SYMV(EXT)	;USE VALUE OF EXTERN (+1 FOR
	400124'	271 01 0 00 000001		ADDI AC,1	;AUTO-INDEXING)
	400125'	202 01 0 00 000004'		MOVEM AC,$ORIGN	;AS POINTER TO CORE.
	400126'	260 17 0 00 000000*		PUSHJ P,$GTWRD	;GET WORD FROM CORE.
	400127'	200 01 0 04 000002		MOVE AC,SYMF(EXT)	;GET FLAGS OF EXTERN REF.
	400130'	622 01 0 00 020000		TRZE AC,GRIPLS	;PLUS REFERENCE?
	400131'	254 00 0 00 400136'		JRST ADDENT	;YES.
	400132'	626 01 0 00 010000		TRZN AC,GRIMIN	;MINUS REFERENCE?
	400133'	254 00 0 00 400143'		JRST NRMEXT	;NO, MUST BE NORMAL EXTERN.
	400134'	274 13 0 03 000001		SUB A,SYMV(ENT)	;SUBTRACT OFF VALUE OF
	400135'	254 00 0 00 400137'		JRST .+2	;ENTRY REFERENCE AND SKIP.
	400136'	270 13 0 03 000001	ADDENT:	ADD A,SYMV(ENT)	;ADD REF... ADD ON VALUE OF REFERENCE.
	400137'	405 13 0 00 177777		ANDI A,177777	;CUT RESULT TO 16 BITS.
	400140'	260 17 0 00 400037*		PUSHJ P,$PTWRD	;STORE RESULT BACK.
	400141'	254 00 0 00 400333'		JRST BADORG	;ERROR, DIE.
	400142'	263 17 0 00 000000	CPOPJ:	POPJ P,	;EXIT.

	400143'	201 01 0 13 000001	NRMEXT:	MOVEI AC,1(A)	;A CONTAINS WORD FROM CORE = NEXT CHAIN POINTER.
	400144'	200 13 0 03 000001		MOVE A,SYMV(ENT)	;REPLACE CORE WORD WITH VALUE
	400145'	260 17 0 00 400140*		PUSHJ P,$PTWRD	;OF ENTRY (IE, THE EXTERN'S DEFINITION).
	400146'	254 00 0 00 400333'		JRST BADORG
	400147'	306 01 0 00 200000		CAIN AC,177777+1	;END OF CHAIN?
	400150'	263 17 0 00 000000		POPJ P,	;YES, PUNT.
	400151'	202 01 0 00 000004'		MOVEM AC,$ORIGN	;NO, CHAIN POINTER IS NEW ORIGIN.
	400152'	260 17 0 00 400126*		PUSHJ P,$GTWRD	;GET NEXT LINK IN CHAIN.
	400153'	254 00 0 00 400143'		JRST NRMEXT	;ONWARD!
GRILNK	GRI-909 LINK EDITOR	MACRO 47(113) 16:15  8-MAR-73 PAGE 5
GLA	MAC	 7-NOV-72 17:51		DICTIONARY PROCESSING

					;GRI-909 RELOCATING LOADER, PAGE 5
					;READ ROUTINES:
					;GET3:  GET REL. FLAG + DATA
					;GETRHD:  GET REL. HEADER
					;RDSYM:  READ SYMBOL AND CONVERT TO MIMIC FORMAT

	400154'	260 17 0 00 400010*	GET3:	PUSHJ P,$RDWRD	;GET 1ST BYTE.
	400155'	263 17 0 00 000000		POPJ P,
	400156'	270 10 0 00 000013		ADD CSUM,ARG	;TREAT AS WHOLE WORD FOR CHECKSUM PURPOSES.
	400157'	200 02 0 00 000013		MOVE RELOC,A
	400160'	260 17 0 00 400154*	RDWRD:	PUSHJ P,$RDWRD	;GET 2ND BYTE.
	400161'	263 17 0 00 000000		POPJ P,
	400162'	200 12 0 00 000013		MOVE ACX,A	;SAVE IT.
	400163'	260 17 0 00 400160*		PUSHJ P,$RDWRD	;GET 3RD BYTE.
	400164'	263 17 0 00 000000		POPJ P,
	400165'	137 12 0 00 400532'		DPB ACX,[POINT 8,A,27]	;FORM COMPLETE WORD FROM BYTE 2 & 3
	400166'	270 10 0 00 000013		ADD CSUM,A	;AND ADD RESULT INTO CHECKSUM.
	400167'	350 00 0 17 000000	CPOPJ1:	AOS ,(P)	;SKIP RETURN.
	400170'	263 17 0 00 000000		POPJ P,

	400171'	260 17 0 00 400160'	GETRHD:	PUSHJ P,RDWRD	;GET RELOCATABLE HEADER.  1ST WORD
	400172'	263 17 0 00 000000		POPJ P,
	400173'	200 10 0 00 000013		MOVE CSUM,A	;IS CHECKSUM.
	400174'	260 17 0 00 400160'		PUSHJ P,RDWRD	;2ND WORD IS BLOCK COUNT
	400175'	263 17 0 00 000000		POPJ P,
	400176'	210 11 0 00 000013		MOVN CNT,A	;WHICH IS NEGATIVE AND MUST BE MADE POSITIVE.
	400177'	405 11 0 00 177777		ANDI CNT,177777
	400200'	254 00 0 00 400167'		JRST CPOPJ1	;EXIT OK.

	400201'	260 17 0 00 400160'	ABSBLK:	PUSHJ P,RDWRD	;ABSOLUTE BLOCK FORMAT IS DIFFERENT.
	400202'	254 00 0 00 400342'		JRST BADBLK
	400203'	210 10 0 00 000013		MOVN CSUM,A	;FIRST WORD IS CHECKSUM, POSITIVE INSTEAD
	400204'	260 17 0 00 400160'		PUSHJ P,RDWRD	;OF NEGATIVE.  2ND WORD IN ORIGIN.
	400205'	254 00 0 00 400342'		JRST BADBLK
	400206'	202 13 0 00 000004'		MOVEM A,$ORIGN
	400207'	260 17 0 00 400160'		PUSHJ P,RDWRD	;NEXT WORD IS BLOCK COUNT, POSITIVE INSTEAD
	400210'	254 00 0 00 400342'		JRST BADBLK
	400211'	200 11 0 00 000013		MOVE CNT,A	;OF NEGATIVE!
	400212'	260 17 0 00 400160'	ABSLP:	PUSHJ P,RDWRD	;GET DATA WORD.
	400213'	254 00 0 00 400342'		JRST BADBLK
	400214'	260 17 0 00 400145*		PUSHJ P,$PTWRD	;STORE IT IN CORE.
	400215'	254 00 0 00 400333'		JRST BADORG
	400216'	350 00 0 00 000004'		AOS ,$ORIGN	;INCREMENT ORIGIN.
	400217'	367 11 0 00 400212'		SOJG CNT,ABSLP	;DECREMENT BLOCK COUNT.  IF NOT ZERO, LOOP.
	400220'	254 00 0 00 400006'		JRST LOADRC	;GO CHECK ON CHECKSUM.
GRILNK	GRI-909 LINK EDITOR	MACRO 47(113) 16:15  8-MAR-73 PAGE 6
GLA	MAC	 7-NOV-72 17:51		DICTIONARY PROCESSING

					;GRI-909 RELOCATING LOADER, PAGE 6
					;
	400221'	260 17 0 00 400160'	RDSYM:	PUSHJ P,RDWRD	;READ SYMBOL ENTRY.  GET FIRST WORD
	400222'	263 17 0 00 000000		POPJ P,
	400223'	200 01 0 00 000013		MOVE AC,A	;AND SAVE.
	400224'	260 17 0 00 400160'		PUSHJ P,RDWRD	;GET 2ND WORD.
	400225'	263 17 0 00 000000		POPJ P,
	400226'	200 07 0 00 000013		MOVE FLGS,A	;AND SAVE AGAIN.
	400227'	306 01 0 00 177777		CAIN AC,177777	;IS "SYMBOL NAME" -1?
	400230'	254 00 0 00 400257'		JRST INDXBL	;YES, INDEXED EXTERNAL EXPRESSION, GO PROCESS.
	400231'	221 01 0 00 003100		IMULI AC,50*50	;1ST AND 2ND WORD ARE RADIX 50 SYMBOL NAMES?
	400232'	405 13 0 00 003777		ANDI A,003777	;MULTIPLY HIGH ORDER 3 DIGITS BY 2500
	400233'	270 01 0 00 000013		ADD AC,A	;AND ADD IN LOWER HALF.
	400234'	260 17 0 00 400265'		PUSHJ P,RDX50C	;CONVERT FROM RADIX 50 TO SIXBIT.
	400235'	260 17 0 00 400160'		PUSHJ P,RDWRD	;GET SYMBOL VALUE.
	400236'	263 17 0 00 000000		POPJ P,
	400237'	200 06 0 00 000013		MOVE VALUE,A	;SAVE IT.

	400240'	405 07 0 00 170000		ANDI FLGS,170000	;CUT OUT EXTRANEOUS FLAG BITS.
	400241'	626 07 0 00 100000		TRZN FLGS,GRIENT	;IS SYMBOL AN ENTRY POINT?
	400242'	254 00 0 00 400263'		JRST EXTFXX	;NO, EXTERN, GO FIX.
	400243'	661 07 0 00 020000		TLO FLGS,QENT	;YES, SET MIMIC ENTRY FLAG.
	400244'	622 07 0 00 020000		TRZE FLGS,GRIPLS	;IS POSITIVE REF BIT SET?
	400245'	254 00 0 00 400254'		JRST NEGENT	;YES, THEN NEGATIVELY RELOCATABLE (UGH).
	400246'	626 07 0 00 040000	CHKREL:	TRZN FLGS,GRIREL	;NO. IS RELOCATION BIT SET?
	400247'	254 00 0 00 400253'		JRST .+4	;NO.
	400250'	270 06 0 00 000005'		ADD VALUE,$RELOC	;YES, ADD ON RELOCATION CONSTANT.
	400251'	405 06 0 00 077777		ANDI VALUE,77777	;CUT RESULT TO 15 BITS.
	400252'	661 07 0 00 001000	ROUND:	TLO FLGS,QREL	;SET MIMIC RELOCATION FLAG.
	400253'	254 00 0 00 400167'		JRST CPOPJ1	;TAKE SKIP RETURN.

	400254'	274 06 0 00 000005'	NEGENT:	SUB VALUE,$RELOC	;NEGATIVE ENTRY POINT.  SUBTRACT
	400255'	405 06 0 00 177777		ANDI VALUE,177777	;CUT RESULT TO 16 BITS.
	400256'	254 00 0 00 400252'		JRST ROUND	;RELOCATION CONSTANT FROM VALUE.

	400257'	200 05 0 00 400421'	INDXBL:	MOVE NAME,INDXNM	;INDEXED EXTERNAL EXPRESSION.
	400260'	260 17 0 00 400160'		PUSHJ P,RDWRD	;CHANGE NAME TO [INDX].
	400261'	263 17 0 00 000000		POPJ P,	;READ THRID WORD (VALUE).
	400262'	334 06 0 00 000013		SKIPA VALUE,A	;SAVE VALUE.
