GLA	MAC	 7-NOV-72 17:51		PARAMETER DEFINITIONS

					;GRI-909 RELOCATING LOADER, PAGE 2
					;MAIN LOADER ROUTINES.
						SUBTTL MAIN ROUTINES

					;$INITU--	INITIALIZE RELOCATION CONSTANT
					;$LOADR--	LOAD A FILE
					;$ELOAD--	PUNCH CORE IMAGE

	400000'	200 01 0 00 000000*	LVLOAD:	MOVE AC,$HI	;AT END OF LOAD, SET NEW RELOCATION CONSTANT
	400001'	271 01 0 00 000001		ADDI AC,1	;TO HIGHEST LOADED WORD+1.
	400002'	354 00 0 17 000000		AOSA ,(P)	;SET UP SKIP RETURN OUT OF LOADER.
	400003'	201 01 0 00 000200	$INITU:	MOVEI AC,DFTORG	;CONSTANT.
	400004'	202 01 0 00 000005'		MOVEM AC,$RELOC
	400005'	263 17 0 00 000000		POPJ P,

	400006'	405 10 0 00 177777	LOADRC:	ANDI CSUM,177777	;AT THE END OF A BLOCK, CHECK
	400007'	326 10 0 00 400324'		JUMPN CSUM,CSUMER	;FOR CHECKSUM ERROR.
	400010'	260 17 0 00 000000*	$LOADR:	PUSHJ P,$RDWRD	;START OF BLOCK... GET A BYTE.
	400011'	254 00 0 00 400000'		JRST LVLOAD	;ON EOF, EXIT.
	400012'	303 13 0 00 000005		CAILE A,MAXCOD	;IS BLOCK CODE LEGAL?
	400013'	254 00 0 00 400342'		JRST BADBLK	;NO, BAD BLOCK FORMAT.
	400014'	254 00 0 13 400015'		JRST CODES(A)	;YES, BRANCH ON BLOCK TYPE.
	400015'	254 00 0 00 400010'	CODES:	JRST $LOADR	;0= FILLER, IGNORE
	400016'	254 00 0 00 400201'		JRST ABSBLK	;1= ABSOLUTE BLOCK
	400017'	254 00 0 00 400000'		JRST LVLOAD	;2= END OF TAPE
	400020'	254 00 0 00 400023'		JRST RELBLK	;3= REL. BLOCK
	400021'	254 00 0 00 400056'		JRST DICTN	;4= ENTRY DICTIONARY
	400022'	254 00 0 00 400056'		JRST DICTN	;5= EXTERN DICTIONARY

	400023'	260 17 0 00 400171'	RELBLK:	PUSHJ P,GETRHD	;REL. BLOCK.  GET BLOCK HEADER.
	400024'	254 00 0 00 400342'		JRST BADBLK	;DIE ON EOF.
	400025'	260 17 0 00 400154'		PUSHJ P,GET3	;GET 3 WORD ADDRESS HEADER
	400026'	254 00 0 00 400342'		JRST BADBLK
	400027'	265600	400047'			RELOCT	;AND RELOCATE.
	400030'	202 13 0 00 000004'		MOVEM A,$ORIGN	;SAVE AS NEW ORIGIN.
	400031'	313 13 0 00 400000*		CAMLE A,$HI	;IF NEW ORIGIN IS GREATER THAN HIGHEST
	400032'	202 13 0 00 400031*		MOVEM A,$HI	;LOADED WORD, SAVE IT AS HIGHEST.
	400033'	254 00 0 00 400042'		JRST CHKCNT	;GO DECREMENT BLOCK COUNT.

	400034'	260 17 0 00 400154'	RELBLP:	PUSHJ P,GET3	;DATA LOOP... GET NEXT DATA WORD.
	400035'	254 00 0 00 400342'		JRST BADBLK
	400036'	265600	400047'			RELOCT	;RELOCATE IT.
	400037'	260 17 0 00 000000*		PUSHJ P,$PTWRD	;PUT WORD IN SIMULATED CORE.
	400040'	254 00 0 00 400333'		JRST BADORG	;ON ERROR, DIE
	400041'	350 00 0 00 000004'		AOS ,$ORIGN	;ADVANCE ORIGIN.
	400042'	367 11 0 00 400034'	CHKCNT:	SOJG CNT,RELBLP	;DECREMENT BLOCK COUNT, LOOP BACK IF NOT DONE.
	400043'	254 00 0 00 400006'		JRST LOADRC	;IF DONE, GO CHECK ON CHECKSUM.

	400044'	255 00 0 00 000000	RELTBL:	JFCL	;REL BYTE=0: ABSOLUTE
	400045'	270 13 0 00 000005'		ADD A,$RELOC	;1:  NORMAL REL.
	400046'	274 13 0 00 000005'		SUB A,$RELOC	;2:  NEGATIVE REL.

	400047'	626 02 0 00 000200	RELSUB:	TRZN RELOC,200	;RELOCATION:  IS INDEX FLAG SET?
